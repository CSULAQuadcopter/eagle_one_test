

<launch>
​
    <!-- don't need a secure mode, listed here for completeness
      <node
      name="secure"
      pkg="eagle_one_test"
      type="secure_mode.py"
      required="true"
      />
    -->
​
    <node
      name="Takeoff"
      pkg="eagle_one_test"
