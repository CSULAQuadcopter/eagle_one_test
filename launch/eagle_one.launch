    <node
       name="TakePicture"
       pkg="eagle_one_test"
       type="take_picture_mode.py"
       required="true"
      />
​
    <node
		   name="Land"
		   pkg="eagle_one_test"
		   type="land_mode.py"
		   required="true"
       />
​
    <node
 		   name="Emergency"
 		   pkg="eagle_one_test"
 		   type="emergency_mode.py"
 		   required="true"
        />
​
    <node
  		  name="Reacquisition"
  		  pkg="eagle_one_test"
  		  type="reacquisition_mode.py"
  		  required="true"
        />

    <node
        name="HUD"
        pkg="eagle_one_test"
        type="hud.py"
        required="true"
        />
